{
	"@context": {
		"epos": "http://epos-eu.org/2016/01/epos-schema#",
		"org": "http://www.w3.org/ns/org#",
		"skos": "http://www.w3.org/2004/02/skos/core#",
		"vacard": "http://www.w3.org/2006/vcard/ns#",
		"dcat": "http://www.w3.org/ns/dcat#",
		"foaf": "http://xmlns.com/foaf/0.1/",
		"cerif" : "http://www.eurocris.org/ontologies/cerif/1.3/",
		"result": {
			"@type": "@id",
			"@id": "@graph"
		},
		"Person": {
			"@type": "@id",
			"@id": "epos:Person"
		},
		"VpVs": "foaf:givenName",
		"Error": "foaf:lastName",
		"Organization": "org:organization",
		"OrgId": "org:identifier",
		"Role": "org:role",
		"Qualification": "foaf:title"
	},
	"result": {% raw result %}
}
